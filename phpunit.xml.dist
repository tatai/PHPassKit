<?xml version="1.0" encoding="UTF-8"?>

<phpunit
	bootstrap="test/bootstrap.php"
	colors="true"
	>
	<testsuites>
		<testsuite name="PHPassKit test suite">
			<directory suffix="Test.php">test</directory>
		</testsuite>
	</testsuites>
	<filter>
		<whitelist>
			<directory suffix=".php">src/PHPassKit</directory>
		</whitelist>
	</filter>
</phpunit>
